{
  "name": "at-decorators",
  "version": "0.0.0-semantically-released",
  "repository": "github:WillBooster/at-decorators",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./src": {
      "require": "./src/index.ts",
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "build-ts lib",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky install",
    "lint": "eslint --color \"./{scripts,src,tests}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "start": "build-ts run src/index.ts",
    "test": "jest tests/unit",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@types/eslint": "8.56.2",
    "@types/jest": "29.5.11",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "@willbooster/eslint-config-ts": "10.5.1",
    "@willbooster/prettier-config": "9.1.2",
    "build-ts": "12.0.4",
    "conventional-changelog-conventionalcommits": "7.0.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-sort-class-members": "1.19.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-unicorn": "50.0.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.2.0",
    "micromatch": "4.0.5",
    "pinst": "3.0.0",
    "prettier": "3.2.2",
    "semantic-release": "22.0.12",
    "sort-package-json": "2.6.0",
    "ts-jest": "29.1.1",
    "typescript": "5.3.3"
  },
  "packageManager": "yarn@4.0.2",
  "publishConfig": {
    "access": "public"
  }
}
